    "\n",
    "# ---------- Oversampling Function ----------\n",
    "def oversample_multilabel(\n",
    "    X, y, labels,\n",
    "    target_labels=(\"threat\", \"identity_hate\"),\n",
    "    multiplier=3,\n",
    "    seed=42\n",
    "):\n",
    "    \"\"\"\n",
    "    Random oversampling for multi-label text classification.\n",
    "    Duplicates training samples that contain rare labels.\n",
    "    \"\"\"\n",
    "\n",
    "    rng = np.random.default_rng(seed)\n",
    "\n",
    "    # Get indices of rare labels\n",
    "    target_idx = [labels.index(lbl) for lbl in target_labels]\n",
    "\n",
    "    # Find samples containing at least one rare label\n",
    "    rare_mask = (y[:, target_idx].sum(axis=1) > 0)\n",
    "    rare_indices = np.where(rare_mask)[0]\n",
    "\n",
    "    print(\"Rare samples before oversampling:\", len(rare_indices))\n",
    "\n",
    "    if len(rare_indices) == 0:\n",
    "        print(\"No rare samples found  oversampling skipped.\")\n",
    "        return X, y\n",
    "\n",
    "    # Sample with replacement\n",
    "    sampled_indices = rng.choice(\n",
    "        rare_indices,\n",
    "        size=len(rare_indices) * multiplier,\n",
    "        replace=True\n",
    "    )\n",
    "\n",
    "    # Concatenate original + oversampled data\n",
    "    X_os = np.concatenate([X, X[sampled_indices]], axis=0)\n",
    "    y_os = np.concatenate([y, y[sampled_indices]], axis=0)\n",
    "\n",
    "    # Shuffle data\n",
    "    perm = rng.permutation(len(X_os))\n",
    "    X_os = X_os[perm]\n",
    "    y_os = y_os[perm]\n",
    "\n",
    "    print(\"Before oversampling:\", X.shape, y.shape)\n",
    "    print(\"After  oversampling:\", X_os.shape, y_os.shape)\n",
    "\n",
    "    return X_os, y_os\n",
    "\n",
    "\n",
    "# ---------- APPLY OVERSAMPLING (TRAIN ONLY) ----------\n",
    "X_train_os, y_train_os = oversample_multilabel(\n",
    "    X_train_pad,          # padded TRAIN input\n",
    "    y_train,              # TRAIN labels\n",
    "    LABELS,               # label names\n",
    "    target_labels=(\"threat\", \"identity_hate\"),\n",
    "    multiplier=3,\n",
    "    seed=42\n",
    ")\n",
    "\n",
    "# ---------- SANITY CHECK ----------\n",
    "print(\"Original training size:\", X_train_pad.shape[0])\n",
    "print(\"Oversampled training size:\", X_train_os.shape[0])\n"
   ]
